<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="enrol/iinterswitch/db" VERSION="20081025" COMMENT="XMLDB file for Moodle enrol/iinterswitch" 
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
<TABLE NAME="enrol_iinterswitch" COMMENT="Holds all known information about iinterswitch transactions">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="20" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="paymentlog_id"/>
        <FIELD NAME="paymentlog_id" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="payment_ref"/>
        <FIELD NAME="payment_ref" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="paymentlog_id" NEXT="courseid"/>
        <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="payment_ref" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="courseid" NEXT="amount"/>
        <FIELD NAME="amount" TYPE="number" LENGTH="50" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="userid" NEXT="payment_status"/>
        <FIELD NAME="payment_status" TYPE="int" LENGTH="2" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="amount" NEXT="payment_date"/>
        <FIELD NAME="payment_date" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="payment_status" />
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" />
      </KEYS>
    </TABLE>     
 
  </TABLES> 

</XMLDB>